<div class = "container">
<div class="panel panel-default">
  <div class="panel-heading">
    <h1>Sign up</h1>
  </div>

  <div class="panel-body">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="form-group">
        <%= f.label :email %>
        <%= f.email_field :email, autofocus: true, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :password %>
        <%= f.password_field :password, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :"What Kind of Learner are you?"%>
        <%= f.password_field :password, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :"What is the last math course that you completed?"%>
        <%= f.password_field :password, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :"How excited are you about learning new  math concepts? (Scale 1-10)"%>
        <%= f.password_field :password, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :"What is a math concept that you would like to strengthen?"%>
        <%= f.password_field :password, class: "form-control" %>
      </div>

  

      <div class="form-group">
        <%= f.submit "Ready to Learn!", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>






  <div class="panel-footer">
    <%= render "devise/shared/links" %>
  </div>
</div>
</div>